{
    "extends": [
        "tslint:latest",
        "tslint-config-prettier",
        "tslint-plugin-prettier"
      ],
    "rules": {
        "prettier": true,
        "no-arg": true,
        "no-bitwise": true,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": false,
        "no-console": false,
        "object-literal-sort-keys": false,
        "no-namespace": false,
        "no-var-requires": false
    }
}
