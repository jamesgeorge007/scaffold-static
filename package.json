{
  "name": "scaffold-static",
  "version": "1.8.6",
  "description": "Start out building static websites powered by vanilla-js with this CLI tool",
  "main": "bin/index.js",
  "files": [
    "bin",
    "lib"
  ],
  "bin": {
    "scaffold-static": "./bin/index.js"
  },
  "scripts": {
    "build": "gulp",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js, ts}": "npm run lint:fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamesgeorge007/scaffold-static.git"
  },
  "keywords": [
    "nodejs",
    "cli",
    "inquirer.js",
    "commander.js"
  ],
  "author": "James George<jamesgeorge998001@gmail.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/jamesgeorge007/scaffold-static/issues"
  },
  "homepage": "https://github.com/jamesgeorge007/scaffold-static#readme",
  "dependencies": {
    "commander": "^5.0.0",
    "execa": "^4.0.0",
    "inquirer": "^6.2.1",
    "kleur": "^3.0.3",
    "mkdirp": "^1.0.3",
    "node-banner": "^1.4.0",
    "ora": "^4.0.3"
  },
  "devDependencies": {
    "@types/inquirer": "^6.0.2",
    "@types/mkdirp": "^0.5.2",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "babel-eslint": "^10.1.0",
    "del": "^5.1.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.2.0",
    "gulp": "^4.0.2",
    "husky": "^2.3.0",
    "lint-staged": "^10.1.1",
    "prettier": "^1.19.1",
    "typescript": "^3.5.1"
  }
}
